{
    "version": 2,
    "settings": {
        "appearanceTools": true,
        "layout": {
            "contentSize": "75rem",
            "wideSize": "87.5rem"
        },
        "typography": {
            "fontFamilies": [
                {
                    "fontFamily": "\"Inter\", -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, Oxygen-Sans, Ubuntu, Cantarell, \"Helvetica Neue\", sans-serif",
                    "name": "Inter",
                    "slug": "primary"
                },
                {
                    "fontFamily": "Monaco, Consolas, \"Andale Mono\", \"DejaVu Sans Mono\", monospace",
                    "name": "Monospace",
                    "slug": "monospace"
                }
            ],
            "fontSizes": [
                {
                    "size": "0.8125rem",
                    "name": "Small",
                    "slug": "small"
                },
                {
                    "size": "0.875rem",
                    "name": "Normal",
                    "slug": "normal"
                },
                {
                    "size": "1rem",
                    "name": "Medium",
                    "slug": "medium"
                },
                {
                    "size": "1.25rem",
                    "name": "Large",
                    "slug": "large"
                },
                {
                    "size": "2rem",
                    "name": "Extra Large",
                    "slug": "x-large"
                }
            ]
        },
        "color": {
            "palette": [
                {
                    "color": "#2271b1",
                    "name": "Primary",
                    "slug": "primary"
                },
                {
                    "color": "#1d2327",
                    "name": "Dark",
                    "slug": "dark"
                },
                {
                    "color": "#f0f0f1",
                    "name": "Light Gray",
                    "slug": "light-gray"
                },
                {
                    "color": "#ffffff",
                    "name": "White",
                    "slug": "white"
                },
                {
                    "color": "#d63638",
                    "name": "Error",
                    "slug": "error"
                },
                {
                    "color": "#00a32a",
                    "name": "Success",
                    "slug": "success"
                },
                {
                    "color": "#dba617",
                    "name": "Warning",
                    "slug": "warning"
                }
            ]
        }
    },
    "styles": {
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--primary)",
            "fontSize": "var(--wp--preset--font-size--normal)",
            "lineHeight": "1.6"
        },
        "elements": {
            "h1": {
                "typography": {
                    "fontSize": "2.5rem",
                    "fontWeight": "600",
                    "lineHeight": "1.3"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "1.75rem",
                    "fontWeight": "600",
                    "lineHeight": "1.3"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "1.25rem",
                    "fontWeight": "600",
                    "lineHeight": "1.3"
                }
            },
            "button": {
                "color": {
                    "text": "var(--wp--preset--color--white)",
                    "background": "var(--wp--preset--color--primary)"
                },
                "border": {
                    "radius": "4px"
                },
                "typography": {
                    "fontWeight": "500"
                }
            },
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--primary)"
                }
            }
        }
    }
}